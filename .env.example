# AWS Configuration
AWS_REGION=us-east-1
AWS_PROFILE=default
S3_BUCKET_RAW=gpu-monitoring-raw-logs-dev
S3_BUCKET_PROCESSED=gpu-monitoring-processed-logs-dev

# InfluxDB Configuration
INFLUXDB_URL=http://localhost:8086
INFLUXDB_TOKEN=your-influxdb-token-here
INFLUXDB_ORG=gpu-monitoring
INFLUXDB_BUCKET=gpu-metrics
INFLUXDB_BUCKET_DAILY=gpu-metrics-daily

# Grafana Configuration
GRAFANA_URL=http://localhost:3000
GRAFANA_USER=admin
GRAFANA_PASSWORD=admin

# GPU-Z Log Configuration
GPU_Z_LOG_PATH=C:\Users\your-username\Documents\projects\GPU-Z Sensor Log.txt
GPU_Z_SIZE_THRESHOLD_MB=5
GPU_Z_CHECK_INTERVAL_SEC=60

# Pipeline Configuration
PROCESSED_DIR=./processed_logs
LAMBDA_TIMEOUT_SEC=120
